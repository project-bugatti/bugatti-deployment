version: "3"
services:
  bugatti:
    image: gcmedia/bugatti:0.1-RELEASE
    ports:
      - "8080:8080"
    volumes:
      - ./logs:/logs
      - ./config:/config
    labels:
labels:
      - "traefik.backend=adminer"
      - "traefik.docker.network=gcmedia-net"
      - "traefik.enable=true"
      - "traefik.protocol=http"
      - "traefik.frontend.rule=Host:adminer.thegc.website"
      - "traefik.port=${ADMINER_PORT}
